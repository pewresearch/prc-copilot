{
  "name": "@automattic/wp-feature-api",
  "version": "0.1.4",
  "description": "Client-side SDK for WordPress Feature API",
  "main": "build/index.js",
  "types": "build-types/index.d.ts",
  "scripts": {
    "build": "wp-scripts build && tsc --emitDeclarationOnly --outDir build-types",
